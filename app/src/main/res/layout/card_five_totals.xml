<android.support.v7.widget.CardView
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:cardCornerRadius="5dp"
    app:cardElevation="3dp"
    app:cardUseCompatPadding="true"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/text_ceil_finish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="4dp"
            android:text="@string/five_eight_regular_no_stretch"
            android:textSize="20sp"
            android:textStyle="bold"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"/>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:columnCount="3"
            android:nestedScrollingEnabled="false"
            android:rowCount="6">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="1"
                android:padding="1.5dp"
                android:text="@string/eight_feet"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="1"
                android:padding="1.5dp"
                android:text="@string/hyphen_with_space"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/fiveE_8_total"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="1"
                android:textIsSelectable="false"
                android:textSize="20sp"
                tools:text="0"
                />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="2"
                android:padding="1.5dp"
                android:text="@string/nine_feet"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="2"
                android:padding="1.5dp"
                android:text="@string/hyphen_with_space"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/fiveE_9_total"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="2"
                android:textIsSelectable="false"
                android:textSize="20sp"
                tools:text="0"
                />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="3"
                android:padding="1.5dp"
                android:text="@string/ten_feet"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="3"
                android:padding="1.5dp"
                android:text="@string/hyphen_with_space"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/fiveE_10_total"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="3"
                android:textIsSelectable="false"
                android:textSize="20sp"
                tools:text="0"
                />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="4"
                android:padding="1.5dp"
                android:text="@string/twelve_feet"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="4"
                android:padding="1.5dp"
                android:text="@string/hyphen_with_space"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/fiveE_12_total"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="4"
                android:textIsSelectable="false"
                android:textSize="20sp"
                tools:text="0"
                />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="5"
                android:padding="1.5dp"
                android:text="@string/fourteen_feet"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="5"
                android:padding="1.5dp"
                android:text="@string/hyphen_with_space"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/fiveE_14_total"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="5"
                android:textIsSelectable="false"
                android:textSize="20sp"
                tools:text="0"
                />

        </GridLayout>


    </LinearLayout>

</android.support.v7.widget.CardView>